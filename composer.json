{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1",
        "fideloper/proxy": "~3.3",
        "krenor/http2-pusher": "^1.0",
        "laravel-frontend-presets/tailwindcss": "^0.4.1",
        "laravel/framework": "5.5.*",
        "league/commonmark": "^0.15.0",
        "spatie/laravel-collection-macros": "^3.0",
        "spatie/laravel-feed": "^2.0",
        "spatie/laravel-responsecache": "^4.0",
        "spatie/laravel-robots-middleware": "^1.0",
        "spatie/yaml-front-matter": "^2.0.0"
    },
    "require-dev": {
        "filp/whoops": "~2.0",
        "graham-campbell/analyzer": "^1.1",
        "laravel/homestead": "^7.1",
        "phpunit/phpunit": "~6.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "files": [
            "app/Support/helpers.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    }
}
